{
  "panthera_blackbox": {
    "version": "1.0.0",
    "site": {
      "domain": "example.com",
      "brand": "Example Brand",
      "verticals": ["general"],
      "geo": ["US"]
    },
    "policy": {
      "privacy_mode": "anon",
      "log_level": "basic"
    },
    "telemetry": {
      "emit": true,
      "keys": [
        "ts.authority",
        "ai.schemaCompleteness",
        "ai.structuredDataQuality",
        "ai.authoritySignals",
        "ai.searchVisibility"
      ],
      "periodic": {
        "enabled": true,
        "intervalMs": 300000
      }
    },
    "authority_grove": {
      "node": {
        "id": "https://example.com",
        "type": "Organization",
        "name": "Example Brand",
        "sameAs": [],
        "keywords": []
      },
      "partners": [],
      "trustEdges": [],
      "corroboration": []
    }
  }
}
