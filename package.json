{
  "name": "gpto-suite",
  "version": "1.1.0",
  "private": true,
  "description": "GPTO Suite - AI Search Optimization Platform with automatic server-side schema injection",
  "repository": {
    "type": "git",
    "url": "https://github.com/careerdriver/gpto-suite"
  },
  "keywords": [
    "ai-search",
    "gpt-optimization",
    "schema.org",
    "json-ld",
    "seo",
    "ai-visibility",
    "structured-data",
    "chatgpt",
    "perplexity",
    "claude",
    "black-box",
    "panthera"
  ],
  "workspaces": [
    "apps/*",
    "packages/*",
    "packages/**"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter dashboard dev",
    "lint": "pnpm -r lint",
    "type-check": "pnpm -r type-check",
    "test": "pnpm -r test",
    "db:migrate": "pnpm --filter database migrate",
    "db:generate": "pnpm --filter database generate",
    "db:seed": "pnpm --filter database seed",
    "install-pnpm": "curl -fsSL https://get.pnpm.io/install.sh | sh"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "strnum": "^1.0.5",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "author": "CareerDriver",
  "license": "UNLICENSED"
}
