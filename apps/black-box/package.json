{
  "name": "@careerdriver/black-box",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/runtime.js",
  "module": "./dist/runtime.js",
  "types": "./dist/runtime.d.ts",
  "exports": {
    ".": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.js"
    },
    "./iife": "./dist/runtime.global.js"
  },
  "unpkg": "./dist/runtime.global.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/runtime.ts --format iife,esm --global-name PantheraBlackBox --minify --sourcemap --dts",
    "dev": "tsup src/runtime.ts --format iife,esm --global-name PantheraBlackBox --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
